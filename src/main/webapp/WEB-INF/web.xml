<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>BudgetServlet</servlet-name>
    <servlet-class>com.ironyard.Servlet.BudgetListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BudgetServlet</servlet-name>
    <url-pattern>/list</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Stats</servlet-name>
    <servlet-class>com.ironyard.Servlet.BudgetStatsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Stats</servlet-name>
    <url-pattern>/stats</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>CreateBudget</servlet-name>
    <servlet-class>com.ironyard.Servlet.BudgetCreateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateBudget</servlet-name>
    <url-pattern>/create</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>com.ironyard.Servlet.BudgetSearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SelectServlet</servlet-name>
    <servlet-class>com.ironyard.Servlet.BudgetSelectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectServlet</servlet-name>
    <url-pattern>/select</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>UpdateServlet</servlet-name>
    <servlet-class>com.ironyard.Servlet.BudgetUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateServlet</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteServlet</servlet-name>
    <servlet-class>com.ironyard.Servlet.BudgetDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteServlet</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>
</web-app>
